<MudPaper Class="pa-5 mb-6" Elevation="2" Style="height: 100%" >
    <MudText Typo="Typo.h6" GutterBottom="true" Color="Color.Surface">@Title</MudText>
    <MudText Typo="Typo.body2">
        @Text
    </MudText>
</MudPaper>


@code {
    [Parameter]
    public string? Text {get; set;}

    [Parameter]
    public string? Title {get; set;}
}
